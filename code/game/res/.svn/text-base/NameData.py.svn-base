#!/usr/bin/env python
# -*- coding:utf-8 -*-

# 名字库	名字
# int&key	string
# id	name


class NameData(object):
    RES_TABLE = "nameConf"

    def __init__(self):
        self.id     = 0 # id
        self.name    = "" # 名字

    def load_from_json(self, data):
        self.id = data.get("id", 0)
        self.name = data.get("name", "")



